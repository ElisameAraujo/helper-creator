{
    "name": "elisame/helper-creator",
    "description": "Custom helper creator for Laravel applications, designed for clean integration and automatic registration.",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Elisame",
            "email": "elisamearaujo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0 || ^12.0"
    },
    "require-dev": {
        "laravel/framework": "^11.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Elisame\\HelperCreator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elisame\\HelperCreator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elisame\\HelperCreator\\HelperCreatorServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
